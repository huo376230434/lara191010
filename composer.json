{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "encore/laravel-admin": "^1.7",
        "fideloper/proxy": "^4.0",
        "huojunhao/lara_admin": "dev-master",
        "huojunhao/lib": "dev-master",
        "laravel/framework": "^6.0",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "huojunhao/dusk_extend": "dev-master",
        "huojunhao/generator": "dev-master",
        "huojunhao/lara_admin_generator": "dev-master",
        "huojunhao/lib_dev": "dev-master",
        "laravel/dusk": "^5.5",
        "laravel/tinker": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./resources/hub/huo-lib"
        },
        {
            "type": "path",
            "url": "./resources/hub/huo-lib_dev"
        },
        {
            "type": "path",
            "url": "./resources/hub/huo-generator"
        },
        {
            "type": "path",
            "url" : "./resources/hub/huo-lara_admin"
        },
        {
            "type": "path",
            "url": "./resources/hub/huo-lara_admin_generator"
        },
        {
            "type": "path",
            "url": "resources/hub/huo-dusk_extend"
        }
    ]
}
